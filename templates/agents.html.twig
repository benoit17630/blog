<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
<main>
    <div class=" text-center row justify-content-center align-items-center ">
        <div class="text-center col-4 mt-4"><img src="{{ asset('assets/img/emilie.jpg') }}"  alt="photo d emilie" class="img-fluid "></div>
        <div class="col-4">
        {% for agent in agents  %}

            {% if agent.published == true %}

                <div><a href="{{ path('agent_show', {id:agent.id}) }}">{{ agent.firstName }} {{ agent.lastName }}</a></div>



            {% endif %}

        {% endfor %}
        </div>

        <div class="text-center col-4"><img src="{{ asset('assets/img/ziva.jpg') }}"  alt="photo d emilie" class="img-fluid "></div>
        <a class="btn btn-primary  mt-4 col-1 offeset-5" href="{{ path('home') }}">page d index</a>
    </div>

    {% for agent in agents|filter(agent=>agent.published) %}
        <p class="text-center mt-4">{{ agent.lastName }} {{ agent.firstName }}</p>

        <p class="text-center">age : {{ agent.age }}</p>
    {% endfor %}
</main>
</body>
</html>
